{
  "name": "Agenda-Anime",
  "version": "1.0.0",
  "description": "agenda anime",
  "main": "main.js",
  "author": {
    "name": "cheark"
  },
  "scripts": {
    "start": "electron ./main.js --disable-gpu",
    "dist": "electron-builder"
  },
  "dependencies": {
    "fs": "^0.0.1-security",
    "jikanjs": "^0.7.0",
    "mariadb": "^2.5.4",
    "path": "^0.12.7",
    "xmlhttprequest": "^1.8.0"
  },
  "devDependencies": {
    "electron": "^15.0.0",
    "electron-builder": "^22.11.7"
  },
  "build": {
    "appId": "com.electron.agenda-anime",
    "productName": "Agenda Anime",
    "target": "nsis",
    "directories": {
      "output": "build",
      "buildResources": "Ressources"
    },
    "nsis": {
      "allowToChangeInstallationDirectory": true,
      "oneClick": false
    }
  }
